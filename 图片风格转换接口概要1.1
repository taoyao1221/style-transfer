/*
* 图片风格转换接口
*/

1. http://api.aihub.finalshares.com:5010/service/aihub/v1/style_transfer/1.0

#传输图片url
{
“platform_uid”:”easemob”,
"uid":"aihub-audit-test-0515001",
"type":"url",
"styles":["wave","scream"],
"urls":["http://url1.jpg","http://url2.jpg"],
“timestamp”:”20170627214109”,
“nonce”:”123457”,
“signature”:”abcdxyz123s.....”
}

response:
{
"urls":["http://url1.jpg","http://url2.jpg"],
"type":0,
"styles":["wave","scream"],
"styled_images":["data:image/jpeg;base64,/9j/4AAQSkZJRgABAAEBLAEsAAD//gAfTEVBRCBUZWNobm9.....","data:image/jpeg;base64,/9j/4AAQSkZJRgABAAEBLAEsAAD//gAfTEVBRCBUZWNobm9....."]
}

or

#传输图片文本数据
{
“platform_uid”:”easemob”,
"uid":"aihub-audit-test-0515001",
"type":"img",
"styles":["wave","scream"],
"urls":["http://url1.jpg","http://url2.jpg"],
"images":["data:image/jpeg;base64,/9j/4AAQSkZJRgABAAEBLAEsAAD//gAfTEVBRCBUZWNobm9.....","data:image/jpeg;base64,/9j/4AAQSkZJRgABAAEBLAEsAAD//gAfTEVBRCBUZWNobm9....."],
“timestamp”:”20170627214109”,
